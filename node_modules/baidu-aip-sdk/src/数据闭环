数据回流任务信息

create table `data_flow_task`(
	`id` bigint(20) unsigned NOT NULL AUTO_INCREMENT COMMENT '主键',
	`api_id` int(10)  NOT NULL DEFAULT 0 COMMENT '回流接口id',
	`name` varchar(50) COMMENT '任务名字',
	`date_type` varchar(4) NOT NULL DEFAULT '' COMMENT '任务回流类型：day,month,hour',
	`input`  varchar(100) COMMENT '数据源',
	`output`  varchar(100) COMMENT '回流输出路径',
	`filter_info`  varchar(100) COMMENT '数据过滤规则',
	`recall` tinyint(1) NOT NULL DEFAULT 0  COMMENT '默认是例行任务，ture是回溯任务',
	`recall_date_start` date  COMMENT '回溯开始日期',
	`recall_date_end` date  COMMENT '回溯结束日期',
	`type` tinyint(4)  NOT NULL DEFAULT 0 COMMENT '1:图片url,2 request esponse 回流，3:pb,字节回流',
	`status` tinyint(1) NOT NULL DEFAULT 1 COMMENT '1，启动，0 关闭',
	PRIMARY KEY (`id`),
    unique key `name` (`name`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='数据回流任务信息' 


create table `task_info`(
	`id` bigint(20) unsigned NOT NULL AUTO_INCREMENT COMMENT '主键',
	`parent_id` bigint(20) NOT NULL DEFAULT -1  COMMENT '任务id',
	`name` varchar(50) COMMENT '任务名字',
	`date_type` varchar(4) NOT NULL DEFAULT '' COMMENT '任务回流类型：day,month,hour',
	`input`  varchar(100) COMMENT '数据源',
	`output`  varchar(100) COMMENT '回流输出路径',
	`status` varchar(4) NOT NULL DEFAULT 'NEW' COMMENT '任务状态',
	`create_time` date DEFAULT NULL  COMMENT '创建时间',
  	`update_time` date DEFAULT NULL  COMMENT '更新时间', 
	PRIMARY KEY (`id`),
    unique key `name` (`name`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='数据回流任务信息';



